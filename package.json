{
  "name": "tsaryk.com",
  "private": true,
  "scripts": {
    "build": "npm run build:prod --",
    "build:dev": "npm run webpack",
    "build:prod": "npm run webpack -- --env.prod",
    "build:prod:relative": "npm run build:prod -- --env.relative",
    "clean": "rimraf ./dist",
    "deploy:dev": "node ./scripts/deploy-dev.js",
    "deploy:prod": "node ./scripts/deploy-prod.js",
    "fix": "npm run eslint -- --fix",
    "format": "prettier --write '**/*.{html,md,json}' && npm run lint:es -- --fix",
    "lint": "run-p lint:*",
    "lint:es": "eslint . --ext .js,.ts,.tsx",
    "lint:types": "tsc --noEmit",
    "prebuild:dev": "npm run clean",
    "prebuild:prod": "npm run clean",
    "serve": "ws --directory ./dist --spa ./index.html --port 8080",
    "start": "npm run start:dev",
    "start:dev": "npm run webpack-dev-server -- --config ./config/webpack.dev.ts",
    "start:dev:external": "npm run start:dev -- -- --host 0.0.0.0",
    "start:prod": "npm run build && npm run serve",
    "test": "jest",
    "webpack": "TS_NODE_PROJECT=config/webpack.tsconfig.json webpack",
    "webpack-dev-server": "TS_NODE_PROJECT=config/webpack.tsconfig.json webpack-dev-server"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "browser-locale": "^1.0.3",
    "date-fns": "^2.9.0",
    "emailjs-com": "^2.4.1",
    "mobx": "^5.15.2",
    "mobx-react": "^6.1.4",
    "rambdax": "^3.5.0",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.0"
  },
  "devDependencies": {
    "@phts/eslint-config": "^2.0.0",
    "@phts/prettier-config": "^1.1.1",
    "@types/history": "^4.7.4",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^16.11.41",
    "@types/ramda": "^0.26.40",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/styled-components": "4.1.8",
    "@types/webpack": "^4.41.3",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-merge": "^4.1.5",
    "awesome-typescript-loader": "^5.2.1",
    "eslint": "^8.18.0",
    "favicons-webpack-plugin": "^3.0.0",
    "file-loader": "^5.0.2",
    "gh-pages": "^2.2.0",
    "html-webpack-plugin": "^4.0.0-beta.11",
    "image-webpack-loader": "^6.0.0",
    "jest": "^25.1.0",
    "jimp": "^0.9.3",
    "local-web-server": "^3.0.7",
    "mockdate": "^2.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "responsive-loader": "^1.2.0",
    "rimraf": "^3.0.0",
    "shell-escape": "^0.2.0",
    "svg-sprite-loader": "^4.1.6",
    "ts-jest": "^25.0.0",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
